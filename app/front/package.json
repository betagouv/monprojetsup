{
  "name": "monprojetsup-front",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "predev": "react-dsfr update-icons",
    "prebuild": "react-dsfr update-icons",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "npx vitest run src --passWithNoTests",
    "test:watch": "npx vitest watch src --passWithNoTests",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:eslint": "eslint --report-unused-disable-directives --max-warnings 0 './**/*.{ts,tsx}'",
    "lint:tsc": "npx tsc",
    "i18n-types": "npx i18next-resources-for-ts interface -i ./src/configuration/i18n/locales/ -o ./src/types/i18next-generated.d.ts",
    "openapi-types": "npx openapi-typescript http://localhost:5002/v3/api-docs -o ./src/types/api-mps.d.ts"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.14.1",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.1",
    "@t3-oss/env-core": "^0.11.1",
    "@tanstack/react-query": "^5.59.14",
    "@tanstack/react-router": "^1.67.0",
    "fuse.js": "^7.0.0",
    "globals": "^15.11.0",
    "i18next": "^23.16.0",
    "oidc-client-ts": "^3.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.0.3",
    "react-oidc-context": "^3.2.0",
    "react-textarea-autosize": "^8.5.4",
    "usehooks-ts": "^3.1.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@hookform/devtools": "^4.3.1",
    "@playwright/test": "^1.48.0",
    "@tanstack/react-query-devtools": "^5.59.14",
    "@tanstack/router-vite-plugin": "^1.66.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-config-canonical": "^44.3.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sonarjs": "^2.0.3",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "sass": "^1.79.5",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0",
    "vite": "^5.4.9",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3",
    "vitest-mock-extended": "^2.0.2"
  }
}
